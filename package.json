{
  "name": "backk",
  "version": "1.0.0-alpha.64",
  "description": "Backk - Modern microservice and backend framework for Node",
  "author": {
    "name": "Petri Silen",
    "email": "petri.silen@silensoft.com"
  },
  "engines": {
    "node": ">= 12.19"
  },
  "license": "MIT",
  "main": "lib/index.js",
  "files": [
    "lib"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/backk-node/backk.git"
  },
  "bugs": {
    "url": "https://github.com/backk-node/backk/issues"
  },
  "keywords": [
    "orm",
    "typescript",
    "microservice",
    "backend",
    "http",
    "kafka",
    "redis",
    "kubernetes",
    "postgresql",
    "mariadb",
    "mysql",
    "mongodb",
    "nest",
    "framework"
  ],
  "scripts": {
    "prebuild": "rimraf build && copyfiles -f \"./src/types/**/*.ts\" lib/src && copyfiles -f \"./src/service/**/*Service.ts\" lib/src",
    "build": "tsc -p tsconfig.json",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "lint": "eslint . --ext .ts",
    "test": "jest --coverage",
    "test:watch": "jest --watch",
    "prepublishOnly": "npm run build"
  },
  "dependencies": {
    "@babel/core": "7.13.15",
    "@babel/generator": "7.13.9",
    "@babel/plugin-proposal-class-properties": "7.13.0",
    "@babel/plugin-proposal-decorators": "7.13.15",
    "@babel/preset-typescript": "7.13.0",
    "@opentelemetry/api": "0.12.0",
    "argon2": "^0.27.2",
    "class-transformer": "0.3.1",
    "class-validator": "0.11.1",
    "cls-hooked": "4.2.2",
    "@types/cls-hooked": "4.3.3",
    "cron": "1.8.2",
    "cron-parser": "2.18.0",
    "csv-parse": "4.15.3",
    "dayjs": "1.10.4",
    "extends-classes": "1.0.5",
    "join-js": "1.1.2",
    "js-base64": "3.6.0",
    "jsonpath-plus": "4.0.0",
    "jsonwebtoken": "8.5.1",
    "libphonenumber-js": "1.9.16",
    "lodash": "4.17.21",
    "mustache": "4.2.0",
    "node-fetch": "2.6.1",
    "organize-imports-cli": "0.7.0",
    "prettier": "1.19.1",
    "randexp": "0.5.3",
    "re2": "1.15.9",
    "reflect-metadata": "0.1.13",
    "typedoc-pksilen": "0.17.4",
    "xmldom": "0.4.0",
    "xpath": "0.0.32",
    "yaml": "1.10.2",
    "yesql": "4.1.3"
  },
  "devDependencies": {
    "@nestjs/common": "6.11.11",
    "@opentelemetry/exporter-jaeger": "0.12.0",
    "@opentelemetry/exporter-prometheus": "0.12.0",
    "@opentelemetry/metrics": "0.12.0",
    "@opentelemetry/node": "0.12.0",
    "@opentelemetry/plugin-dns": "0.11.0",
    "@opentelemetry/plugin-http": "0.12.0",
    "@opentelemetry/plugin-https": "0.12.0",
    "@opentelemetry/plugin-ioredis": "0.11.0",
    "@opentelemetry/plugin-mongodb": "0.11.0",
    "@opentelemetry/plugin-mysql": "0.11.0",
    "@opentelemetry/plugin-pg": "0.11.0",
    "@opentelemetry/plugin-pg-pool": "0.11.0",
    "@types/cron": "1.7.2",
    "@types/express": "4.17.11",
    "@types/ioredis": "4.22.3",
    "@types/jest": "25.1.2",
    "@types/jsonwebtoken": "8.5.1",
    "@types/kafka-node": "3.0.0",
    "@types/lodash": "4.14.168",
    "@types/mongodb": "3.6.12",
    "@types/mustache": "4.1.1",
    "@types/node": "13.13.48",
    "@types/node-fetch": "2.5.10",
    "@types/pg": "7.14.11",
    "@types/xmldom": "0.1.30",
    "@types/yesql": "3.2.2",
    "@typescript-eslint/eslint-plugin": "2.34.0",
    "@typescript-eslint/parser": "2.34.0",
    "copyfiles": "^2.4.1",
    "eslint": "6.8.0",
    "eslint-config-prettier": "6.15.0",
    "eslint-plugin-import": "2.22.1",
    "ioredis": "4.26.0",
    "jest": "24.9.0",
    "kafkajs": "1.15.0",
    "mongodb": "3.6.6",
    "mysql2": "2.2.5",
    "pg": "8.5.1",
    "rimraf": "3.0.2",
    "ts-jest": "25.2.0",
    "ts-loader": "6.2.2",
    "ts-node": "8.10.2",
    "tsconfig-paths": "3.9.0",
    "typescript": "3.9.9"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": "src",
    "testRegex": ".spec.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "coverageDirectory": "../coverage",
    "testEnvironment": "node"
  }
}
